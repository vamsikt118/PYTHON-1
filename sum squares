a=int(input("enter the values"))
x=[]
if a>0:
    while a>=1:
        i=int(input("enter your element"))
        x.append(i)
        a-=1
        even=0
        odd=0
    for k in x:
        if k%2==0:
            even+=k**2
        else:
            odd+=k**2
    print("(",odd,",",even,")")
else:
    print("invalid input")
